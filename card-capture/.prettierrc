{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "proseWrap": "never",
  "endOfLine": "auto",
  "keepEmptyLines": true,
  "maxEmptyLines": 3,
  "plugins": [
    "prettier-plugin-tailwindcss"
  ],
  "css": {
    "ignoreFiles": "*.css"
  }
}
